import xml.etree.ElementTree as ET


def extract_c4_names(drawio_file_path):
    with open(drawio_file_path, 'r', encoding='utf-8') as f:
        content = f.read()

    root = ET.fromstring(content)

    # üîΩ –ü–µ—á–∞—Ç–∞–µ–º –≤—Å–µ –∏–º–µ–Ω–∞ –ª–∏—Å—Ç–æ–≤ (–µ—Å–ª–∏ –æ–Ω–∏ –µ—Å—Ç—å)
    diagrams = root.findall("diagram")
    if diagrams:
        print("üìÑ –õ–∏—Å—Ç—ã –≤ —Ñ–∞–π–ª–µ:")
        for diagram in diagrams:
            name = diagram.attrib.get("name", "(–±–µ–∑ –∏–º–µ–Ω–∏)")
            print(f"- {name}")
        print("")  # –ø—É—Å—Ç–∞—è —Å—Ç—Ä–æ–∫–∞-—Ä–∞–∑–¥–µ–ª–∏—Ç–µ–ª—å

    # üîç –ü–æ–∏—Å–∫ –≤—Å–µ—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤ —Å –∞—Ç—Ä–∏–±—É—Ç–æ–º c4Technology
    c4_names = []
    for elem in root.iter():
        if 'c4Technology' in elem.attrib:
            c4_names.append(elem.attrib['c4Technology'])

    return c4_names
